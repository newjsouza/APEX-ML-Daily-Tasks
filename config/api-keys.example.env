# APEX-ML API Configuration
# Copie este arquivo para api-keys.env e preencha com suas chaves reais

# Perplexity AI
PERPLEXITY_API_KEY=your_perplexity_key_here

# Google Gemini
GEMINI_API_KEY=your_gemini_key_here

# SofaScore (se necessário)
SOFASCORE_API_KEY=your_sofascore_key_here

# Understat (se necessário)
UNDERSTAT_API_KEY=your_understat_key_here

# Configurações de Análise
MIN_CONFIDENCE=0.70
MAX_DAILY_BETS=10
DEFAULT_STAKE=100

# Modo de Operação
MODE=production
DEBUG=false
LOG_LEVEL=info